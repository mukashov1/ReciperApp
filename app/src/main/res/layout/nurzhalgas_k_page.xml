        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="383dp"
        android:layout_height="320dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="202dp"
        android:layout_marginBottom="71dp"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar3"
        app:layout_constraintEnd_toStartOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar"
        app:srcCompat="@drawable/classic_fluffy_pancakes_5" />

    <RatingBar
        android:id="@+id/ratingBar3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="863dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="412dp"
        android:layout_height="230dp"
        app:srcCompat="@drawable/daylight"
        tools:layout_editor_absoluteX="628dp"
        tools:layout_editor_absoluteY="549dp" />

    <RatingBar
        android:id="@+id/ratingBar4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="697dp"
        tools:layout_editor_absoluteY="853dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="261dp"
        android:layout_height="284dp"
        app:srcCompat="@drawable/download"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="1047dp" />

    <RatingBar
        android:id="@+id/ratingBar5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="1403dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="277dp"
        android:layout_height="328dp"
        app:srcCompat="@drawable/untitled_design_20"
