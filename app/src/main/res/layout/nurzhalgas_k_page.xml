        tools:layout_editor_absoluteY="1047dp" />

    <RatingBar
        android:id="@+id/ratingBar6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="673dp"
        tools:layout_editor_absoluteY="1426dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="383dp"
        android:layout_height="122dp"
        android:layout_marginStart="303dp"
        android:layout_marginTop="167dp"
        android:layout_marginEnd="394dp"
        android:layout_marginBottom="123dp"
        android:text="  Load More"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar6" />
</androidx.constraintlayout.widget.ConstraintLayout>